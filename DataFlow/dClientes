                                    "31 - 40 ANOS",
                                    "41 - 50 ANOS",
                                    "50 - 60 ANOS",
                                    "MAIS DE 60 ANOS"
                                }
                        in
                            Resultados{List.PositionOf(Condicoes,true)}
                in
                    Faixas, type text
            ),
            e5 = Table.AddColumn(e4,"Renda", each 
                let 
                    Salarios = Number.RoundDown(Number.RandomBetween(0,10)),
                    Faixas = 
                        let
                            Condicoes = 
                                {
                                    Salarios < 1,
