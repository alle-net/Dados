                                    "ENTRE 1 e 2 SALARIOS",
                                    "ENTRE 3 e 4 SALARIOS",
                                    "ENTRE 5 E 9 SALARIOS",
                                    "MAIS DE 10 SALARIOS"
                                }
                        in
                            Resultados{List.PositionOf(Condicoes,true)}
                in
                    Faixas, type text
            )
        in
            e5
in
    Import
