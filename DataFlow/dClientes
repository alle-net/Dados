            e4 = Table.AddColumn(e3,"FaixaEtaria", each 
                let 
                    Data = Date.From(DateTimeZone.LocalNow()),
                    Dias = Date.AddDays(Data, Number.Round(Number.RandomBetween(-31,0))),
                    Meses = Date.AddMonths(Dias, Number.Round(Number.RandomBetween(-12,0))),
                    Aniv = Date.AddYears(Meses,Number.Round(Number.RandomBetween(-70,-18))),
                    Dif = Duration.Days(Data - Aniv) + 1,
                    Idade = Number.RoundDown(Value.Divide(Dif,365)),

                    Faixas = 
                        let
                            Condicoes = 
                                {
                                    Idade <= 21,
                                    Idade > 21 and Idade <= 25,
                                    Idade > 25 and Idade <= 30,
                                    Idade > 30 and Idade <= 40,
                                    Idade > 40 and Idade <= 50,
                                    Idade > 50 and Idade <= 60,
                                    Idade > 60
                                },
